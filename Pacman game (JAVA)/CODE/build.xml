<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create Runnable Jar for Project team-9">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <target name="create_run_jar">
        <jar destfile="./bin/Pacman.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Main-Class" value="display.WindowManager"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="./bin"/>
            <zipfileset excludes="META-INF/*.SF" src="./lib/junit-4.10.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="./lib/miglayout15-swing.jar"/>
        </jar>
    </target>
</project>
